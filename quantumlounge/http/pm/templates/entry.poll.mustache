<div class="activity" id="a-{{ id }}">
    <div class="body">
        <span class="title">{{ content }}</span>
        <ul class="answers">
        {{#answers}}
          <li>{{.}}</li>
        {{/answers}}
        
        </ul>
    </div>
    <div class="meta">
        <span class="fullname"><a href="#">{{ username }}</a></span>
        <span class="time">{{ date }}</span>
    </div>
</div>
